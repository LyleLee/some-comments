<h1>You are logged in as {{user}}!</h1>

<script>
  console.log('Sending message to:', window.parent, window.opener)
  if (window.opener) {window.opener.postMessage({authenticated: true}, '*')}
  if (window.parent) {window.parent.postMessage({authenticated: true}, '*')}
  console.log('Sent message to:', window.parent, window.opener)
  window.close()
</script>
